package demomain;

public class Ex01DemoMainMethod {
	public static void main(String[] args) {
		//
		System.out.println("1. Class name");
		System.out.println("2. Main method");
		
		System.out.println("\b================");
		//\b  \t  \n  \f  \r  \"  \'  \\
		System.out.println("*");
		System.out.println("**");
		System.out.println("***");
		System.out.println("****");
		System.out.println("*****");
	}
}
